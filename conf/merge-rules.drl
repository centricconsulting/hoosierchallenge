import util.CCDHelper
import service.MergedCCD

# Merge entire Encounters section
rule "Merge entire Encounters"
@RuleVersion("Merge entire Encounters 0.1")
salience 1000
when
  m : MergedCCD()
  h : CCDHelper(h.findSectionByTitle("Encounters").isEmpty() == false)
then
  m.helper().document().addSection(h.findSectionByTitle("Encounters").get());
end
